package domain.exception;

public class FormatoDeFechaIncorrectoException extends RuntimeException
{

	public FormatoDeFechaIncorrectoException()
	{
		
	}
	
	public FormatoDeFechaIncorrectoException(Throwable ex)
	{
		super(ex);
	}

	@Override
	public String getMessage()
	{
		return "El formato de fecha ingresado es incorrecto, debe ser dd/MM/aaaa";
	}

}
